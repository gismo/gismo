# Copy compiler information from C++ compiler.

set(CMAKE_CXXPCH_COMPILER "@CMAKE_CXX_COMPILER@")
set(CMAKE_CXXPCH_COMPILER_ARG1 "@CMAKE_CXX_COMPILER_ARG1@")
set(CMAKE_CXXPCH_COMPILER_ID "@CMAKE_CXX_COMPILER_ID@")
set(CMAKE_CXXPCH_COMPILER_VERSION "@CMAKE_CXX_COMPILER_VERSION@")
set(CMAKE_CXXPCH_PLATFORM_ID "@CMAKE_CXX_PLATFORM_ID@")
set(CMAKE_CXXPCH_SIMULATE_ID "@CMAKE_CXX_SIMULATE_ID@")
set(CMAKE_CXXPCH_SIMULATE_VERSION "@CMAKE_CXX_SIMULATE_VERSION@")
@SET_MSVC_CXXPCH_ARCHITECTURE_ID@
set(CMAKE_CXXPCH_COMPILER_LOADED 1)
set(CMAKE_CXXPCH_COMPILER_WORKS @CMAKE_CXX_COMPILER_WORKS@)
set(CMAKE_CXXPCH_ABI_COMPILED @CMAKE_CXX_ABI_COMPILED@)

set(CMAKE_CXXPCH_COMPILER_ENV_VAR "CXXPCH")

set(CMAKE_CXXPCH_COMPILER_ID_RUN 1)

# Save compiler ABI information.
set(CMAKE_CXXPCH_SIZEOF_DATA_PTR "@CMAKE_CXX_SIZEOF_DATA_PTR@")
set(CMAKE_CXXPCH_COMPILER_ABI "@CMAKE_CXX_COMPILER_ABI@")
set(CMAKE_CXXPCH_LIBRARY_ARCHITECTURE "@CMAKE_CXX_LIBRARY_ARCHITECTURE@")

@CMAKE_CXXPCH_SYSROOT_FLAG_CODE@
@CMAKE_CXXPCH_OSX_DEPLOYMENT_TARGET_FLAG_CODE@
